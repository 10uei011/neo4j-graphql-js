{
  "name": "neo4j-graphql-js",
  "version": "0.1.0",
  "description": "A GraphQL to Cypher query execution layer for Neo4j. ",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon ./example/graphql-tools/movies.js --exec babel-node -e js",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "test": "ava test/*.js"
  },
  "author": "William Lyon",
  "license": "Apache 2.0",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.23.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "graphql": "^0.11.3",
    "graphql-server-core": "^1.1.0",
    "graphql-server-express": "^0.6.0",
    "graphql-tools": "^1.2.2",
    "lodash": "^4.17.4",
    "neo4j-driver": "^1.4.0"
  }
}
